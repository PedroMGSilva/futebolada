{
  "name": "futebolada",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "node ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "react-router typegen && tsc",
    "migrate": "node-pg-migrate up -j js"
  },
  "dependencies": {
    "@react-router/express": "^7.7.1",
    "@react-router/node": "^7.7.1",
    "bcrypt": "^6.0.0",
    "cross-env": "^10.0.0",
    "express": "^5.1.0",
    "isbot": "^5.1.27",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pg-migrate": {
    "dir": "migrations",
    "migrations-table": "pgmigrations",
    "database-url-env": true,
    "tsconfig": "./tsconfig.json"
  }
}
